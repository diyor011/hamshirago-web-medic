{
  "expo": {
    "name": "HamshiraGo",
    "slug": "hamshira-go",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "hamshiragomobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#f8fafc"
    },
    "ios": { "supportsTablet": true },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#f8fafc"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      ["expo-location", { "locationWhenInUsePermission": "HamshiraGo использует геолокацию для точного адреса вызова медсестры." }],
      ["expo-notifications", { "icon": "./assets/images/icon.png", "color": "#0d9488", "sounds": [] }]
    ],
    "experiments": { "typedRoutes": true }
  }
}
